{
    "include": ["src/assets/js/**/*.ts", "src/demo/assets/js/**/*.ts"],
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "node",
        "target": "esnext",
        "jsx": "preserve",
    
        "resolveJsonModule": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "verbatimModuleSyntax": true,
        "skipLibCheck": true,
        "strictNullChecks": false,
      }
}
{
    "include": ["src/assets/js/**/*.ts", "src/demo/assets/js/**/*.ts"],
    "exclude": ["src/demo/assets/js/demo.ts"],
    "compilerOptions": {
        "target": "ES6",
        "moduleResolution": "Node",
        "module": "ESNext",
        // "noEmit": false,
        "forceConsistentCasingInFileNames": true,
        "noUnusedParameters": false,
        "strictNullChecks": false,
        "esModuleInterop": true,
        "declaration": true,
        "declarationDir": "."
    }
}